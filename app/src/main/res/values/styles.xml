<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="FormEditTitle">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">#ff000000</item>
    </style>
    <style name="FormEditContent">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">#96494747</item>
    </style>
    <style name="iconTitle">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">#96494747</item>
    </style>
    <style name="FormEditValue">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ff000000</item>
    </style>
    <style name="InputMoney">
        <item name="android:textSize">30.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ff000000</item>
    </style>
    <style name="InputMoneyKeyboard">
        <item name="android:textSize">33.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ff000000</item>
    </style>
    <style name="TextViewStyle">
        <item name="android:textSize">19.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="TextViewStyleBold">
        <item name="android:textSize">22.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="TextViewStyleSpinner">
        <item name="android:textSize">20.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="KeyPassCode">
        <item name="android:textSize">30.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="TextHeader">
        <item name="android:textSize">30.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
</resources>
